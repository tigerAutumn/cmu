    <parent>
        <groupId>cc.newex.dax</groupId>
        <artifactId>newex-extra</artifactId>
        <version>1.0.1-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <artifactId>newex-extra-service</artifactId>
