    <description>newex交易平台-辅助服务项目-核心业务模块</description>
    <dependencies>
        <dependency>
            <groupId>cc.newex.dax</groupId>
            <artifactId>newex-extra-dto</artifactId>
        </dependency>
        <!-- newex -->
        <dependency>
            <groupId>cc.newex.commons</groupId>
            <artifactId>newex-mybatis-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>cc.newex.commons</groupId>
            <artifactId>newex-fileupload-spring-boot-starter</artifactId>
